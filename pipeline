pipeline {
    agent any

    stages {
        stage('Build') {
            steps {
                sh("chmod +x ./test.sh | ./test.sh")
            }
        }
        stage('Test') {
            steps {
                echo 'Testing....'
            }
        }
        stage('Deploy') {
            steps {
                echo 'Deploying....'
            }
        }
    }
}
